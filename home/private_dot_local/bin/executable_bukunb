#!/bin/sh

url="$1"
title="$2"
# description="$3"
# feed_title="$4"

printf "Title: %s\nURL: %s\n" "${title}" "${url}"
printf "Enter some tags comma seperated: "; read -r tags

if [ ! "$tags" ]; then
  tags="newsboat"
fi

buku --nostdin -a "$url" "$tags" --title "$title" >/dev/null || printf "Bookmark Existed"
